let a = 5

let override = false
let underride = true

if (a >= 0 || override) {
    print "true || false"
}

if (a <= 0 || override) {
    print "false || false"
}

if (a >= 0 || underride) {
    print "true || true"
}

if (a <= 0 || underride) {
    print "false || true"
}

if (a >= 0 && override) {
    print "true && false"
}

if (a <= 0 && override) {
    print "false && false"
}

if (a >= 0 && underride) {
    print "true && true"
}

if (a <= 0 && underride) {
    print "false && true"
}